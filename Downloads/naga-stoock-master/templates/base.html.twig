<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %} {{'title.website'| trans}} {% endblock %}</title>
        {% block stylesheets %} 
            <link href="{{ asset('css/style.css') }}" rel="stylesheet">
            <link href="https://fonts.googleapis.com/css2?family=Mulish:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Open+Sans:ital,wght@0,300;0,400;0,600;0,700;0,800;1,300;1,400;1,600;1,700;1,800&display=swap" rel="stylesheet">
            <link rel="stylesheet" href="{{ asset('css/font-awesome/css/all.min.css') }}">       
        {% endblock %}
    </head>
    <body>
        <div class="wb-home">
            {% include "menu.html.twig" %}
            <div class="container" role="main" aria-label="Main" aria-describedby="Main content">
                {% include "header.html.twig" %}
                {% block body %}
                {% endblock %}
            </div>
        </div>

        {% block javascripts %}
            <script src="{{ asset('js/app.js') }}"></script>
        {% endblock %}
    </body>
</html>
